{
    "name": "anthonypauwels/metadata",
    "type": "library",
    "description": "An helper package to build metadata tags on PHP pages",
    "keywords": [
        "metadata",
        "tags",
        "helper"
    ],
    "authors": [
        {
            "name": "Anthony Pauwels",
            "email": "hello@anthonypauwels.be",
            "homepage": "https://anthonypauwels.be/"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "anthonypauwels\\Metadata\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anthonypauwels\\Metadata\\Laravel\\ServiceProvider"
            ],
            "aliases": {
                "Metadata": "Anthonypauwels\\Metadata\\Laravel\\Metadata"
            }
        }
    }
}
